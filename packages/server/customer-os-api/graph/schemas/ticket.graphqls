type Ticket implements Node {
    id: ID!
    createdAt: Time!
    updatedAt: Time!

    subject: String
    status: String
    priority: String
    description: String
    tags: [Tag] @goField(forceResolver: true)
    notes: [Note] @goField(forceResolver: true)
}

type TicketSummaryByStatus {
    status: String!
    count: Int64!
}
