union Action = PageViewAction | InteractionSession | Ticket

type PageViewAction implements Node {
    id: ID!
    startedAt: Time!
    endedAt: Time!
    pageTitle: String!
    pageUrl: String!
    application: String!
    sessionId: ID!
    orderInSession: Int64!
    engagedTime: Int64!
}

enum ActionType {
    PAGE_VIEW
    INTERACTION_SESSION
    TICKET
}