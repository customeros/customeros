type Action {
    id: ID!
    createdAt: Time!
    content: String
    source: DataSource!
    appSource: String!
    createdBy: User
    actionType: ActionType!
    metadata: String
}

enum ActionType {
    CREATED
    RENEWAL_LIKELIHOOD_UPDATED
    RENEWAL_FORECAST_UPDATED
    CONTRACT_STATUS_UPDATED
    SERVICE_LINE_ITEM_PRICE_UPDATED
    SERVICE_LINE_ITEM_QUANTITY_UPDATED
}