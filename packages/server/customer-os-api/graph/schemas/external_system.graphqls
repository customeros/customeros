input ExternalSystemReferenceInput {
    externalId: ID!
    syncDate: Time
    type: ExternalSystemType!
    externalUrl: String
    externalSource: String
}

enum ExternalSystemType {
    HUBSPOT
    ZENDESK_SUPPORT
    CALCOM
    PIPEDRIVE
    SLACK
    INTERCOM
    SALESFORCE
    STRIPE
    MIXPANEL
}

type ExternalSystem {
    type: ExternalSystemType!
    syncDate: Time
    externalId: String
    externalUrl: String
    externalSource: String
}