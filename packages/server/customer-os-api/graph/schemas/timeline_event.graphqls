union TimelineEvent = PageView | InteractionSession | Conversation | Note | InteractionEvent | Analysis | Issue | Meeting | Action

extend type Query {
    timelineEvents(ids: [ID!]!): [TimelineEvent!]!
}

enum TimelineEventType {
    PAGE_VIEW
    INTERACTION_SESSION
    CONVERSATION
    NOTE
    INTERACTION_EVENT
    ANALYSIS
    ISSUE
    MEETING
}