input CompanyInput {

    id: ID

    """
    The name of the company associated with a Contact.
    **Required.**
    """
    name: String
}

type Company {
    id: ID!
    name: String!
}

type CompanyPage implements Pages {
    content: [Company!]!
    totalPages: Int!
    totalElements: Int64!
}

extend type Query {
    companies_ByNameLike(pagination: Pagination, companyName: String!): CompanyPage
}

